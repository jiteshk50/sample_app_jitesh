<% @page_title = "Edit Exam Type: #{@exam_type.exam_name}" -%>

<%= link_to("<< Back to List", {:action => 'index'}) %>

<div >
  <h2>Edit Exam Type</h2>

  <%= form_for(:exam_type, :url => {:action => 'update', :id => @exam_type.id}) do |f| %>

    <%= render(:partial => 'form', :locals => {:f => f} ) %>

    <div>
      <%= submit_tag('Update Exam Type', :class => "btn btn-warning") %>
    </div>
  <% end %>
</div>
